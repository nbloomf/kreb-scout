[define >doc @long.term.asymptote.1 := Macro(doc; [#

[define {int} @cs := Choose(3; {1;2;3;-1;-2;-3})]#
[define {int} @ds := Choose(1; {4;5;6;-4;-5;-6})]#

[define ^int @p := FromRoots(x; @cs)]#
[define ^int @q := FromRoots(x; @ds)]#

Find the long-term behavior asymptote of the rational function \#[ f(x) = \frac{[:str: Format(^int; latex; @p) :]}{[:str: Format(^int; latex; @q) :]}. \#]#

  ]
)]#



